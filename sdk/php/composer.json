{
  "name": "workersql/php-sdk",
  "description": "PHP SDK for WorkerSQL - MySQL at the edge on Cloudflare",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "mysql",
    "database",
    "edge",
    "cloudflare",
    "sql",
    "workersql",
    "pdo",
    "mysqli"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "HealthFees Organization",
      "email": "developers@healthfees.org"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.6",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WorkerSQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WorkerSQL\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs --standard=PSR12 src tests",
    "analyze": "phpstan analyse src --level=5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
